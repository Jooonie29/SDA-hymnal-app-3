{
  "expo": {
    "name": "SDA Hymnal",
    "slug": "sda-hymnal",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "plugins": [
      ["react-native-track-player", { "android": { "appKilledPlaybackBehavior": "continuePlayback" } }]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    }
  }
}

